{
  "name": "commentup-landing",
  "description": "CommentUp landing page in TypeScript/Next.js!",
  "version": "1.0.0",
  "private": true,
  "author": "Andres Felipe Ceballos",
  "license": "MIT",
  "keywords": [
    "commentup",
    "social media",
    "instagram",
    "tiktok",
    "youtube",
    "facebook"
  ],
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install",
    "ingest": "tsx -r dotenv/config src/scripts/ingest-data.ts"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@emotion/css": "^11.10.0",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@fontsource/inter": "4.2.1",
    "@mdi/js": "^7.0.96",
    "@pinecone-database/pinecone": "0.0.12",
    "@radix-ui/react-accordion": "^1.1.1",
    "@reduxjs/toolkit": "^1.8.5",
    "@svgr/webpack": "5.5.0",
    "@tailwindcss/forms": "^0.5.2",
    "animate.css": "^4.1.1",
    "autoprefixer": "^10.4.0",
    "axios": "1.3.4",
    "chart.js": "^3.9.1",
    "cookie": "0.5.0",
    "dotenv": "^16.0.3",
    "escape-html": "1.0.3",
    "firebase": "9.19.1",
    "formik": "^2.2.9",
    "is-hotkey": "0.2.0",
    "leaflet": "1.9.4",
    "next": "13.1.2",
    "next-seo": "4.20.0",
    "numeral": "^2.0.6",
    "openai": "3.2.1",
    "pdf-parse": "1.1.1",
    "pdfjs-dist": "3.5.141",
    "react": "18.2.0",
    "react-chartjs-2": "^4.3.1",
    "react-cookie": "4.1.1",
    "react-dom": "18.2.0",
    "react-markdown": "8.0.7",
    "react-phone-input-2": "2.15.1",
    "react-redux": "^8.0.2",
    "react-share": "4.4.1",
    "react-type-animation": "3.1.0",
    "slate": "0.93.0",
    "slate-react": "0.93.0",
    "swr": "^1.3.0",
    "tailwindcss": "^3.3.0",
    "twind": "0.15.4",
    "yup": "1.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@types/escape-html": "1.0.2",
    "@types/node": "14.14.28",
    "@types/numeral": "2.0.2",
    "@types/pdf-parse": "1.1.1",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.1",
    "@types/slate-html-serializer": "0.6.5",
    "@typescript-eslint/eslint-plugin": "4.15.1",
    "@typescript-eslint/parser": "4.15.1",
    "commitizen": "4.2.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.20.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "5.0.9",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "tsx": "^3.12.3",
    "typescript": "4.1.5"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
